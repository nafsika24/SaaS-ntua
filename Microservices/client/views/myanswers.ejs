<link rel="stylesheet" href="/css/allquestions.css">
<link rel="stylesheet" href="/css/forms.css">
<link rel="stylesheet" href="/css/top-navbar.css">
</head>

<body>
<%- include('includes/navigation.ejs') %>

<main>
    <h2 class = "header44"> Your Answers</h2>
    <div>
        <% if (quest.length > 0) { %>
        <h3 class="center1"> <b>Total Answers: </b><%=quest.length %>  </h3>
        <% for(let question of quest) { %>
        <div class = "container1">
            <p class="groove"> <b>Answer: </b><%=question.text %>  </p>
        </div>
        <% }%>
        <% }
        else{ %>
            <h2> No answers yet!</h2>
        <% }%>
    </div>
</main>

<%- include('includes/end.ejs') %>